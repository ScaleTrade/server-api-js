# ===========================
# Files to exclude from npm package
# ===========================

# Development dependencies
node_modules/
package-lock.json

# Logs
*.log
logs/

# OS-specific files
.DS_Store
Thumbs.db

# IDE/editor settings
.idea/
.vscode/

# Tests and coverage
test/
tests/
__tests__/
coverage/
*.lcov

# Environment and local config files
.env*
*.local

# Build artifacts
.tmp/
.temp/

# Source maps
*.map

# Git files
.git/
.github/
.gitignore

# NPM/Yarn debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Files usually not needed in a package
*.md.tmp
*.swp
*.swo

# Exclude documentation generators or configs
docs/
.storybook/

# Local scripts or tooling not intended for consumers
scripts/
tools/